        "svgdom": "0.1.8",
        "ts-jest": "26.5.4",
        "ts-loader": "8.1.0",
        "webpack": "5.32.0",
        "webpack-bundle-analyzer": "4.4.0",
        "webpack-cli": "4.6.0",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "5.7.3"
