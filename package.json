{
    "name": "roadmap-radar",
    "version": "0.0.1",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "yarn start:development",
        "start:development": "yarn webpack serve --config webpack.dev.js",
